{
  "name": "git-a-readme",
  "version": "1.2.5",
  "description": "CLI Application that dynamically generates a professional git README. Information from package.json is used to help pre-populate responses if file exists. This project's README was generated using the node application.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint ./ --ext js,jsx --report-unused-disable-directives --max-warnings 10",
    "lint-fix": "eslint ./ --ext js,jsx --report-unused-disable-directives --max-warnings 10 --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shindekokoro/git-a-readme.git"
  },
  "keywords": [
    "GIT",
    "README",
    "NODE",
    "NPM",
    "macOS",
    "linux",
    "raspberryPI"
  ],
  "author": "shindekokoro (Brian Whisler)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shindekokoro/git-a-readme/issues"
  },
  "homepage": "https://www.npmjs.com/package/git-a-readme",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "git-a-readme": "^1.2.5",
    "prettier": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "inquirer": "^8.2.4"
  }
}